{% if content.content.link %}
  {{ macros.linkStart(content.content.link, 'dan-dynamic-copyright__anchor') }}
    {% if content.content.prefix %}
      <span class='dan-dynamic-copyright__prefix'>{{ content.content.prefix }}</span>
    {% endif %}
    <span class='dan-dynamic-copyright__year'>20??</span>
    {% if content.content.suffix %}
      <span class='dan-dynamic-copyright__suffix'>{{ content.content.suffix }}</span>
    {% endif %}
  {{ macros.linkEnd() }}
{% else %}
  {% if content.content.prefix %}
    <span class='dan-dynamic-copyright__prefix'>{{ content.content.prefix }}</span>
  {% endif %}
  <span class='dan-dynamic-copyright__year'>20??</span>
  {% if content.content.suffix %}
    <span class='dan-dynamic-copyright__suffix'>{{ content.content.suffix }}</span>
  {% endif %}
{% endif %}