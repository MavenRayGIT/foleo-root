%%SELECTOR%%{
    white-space: {{ content.content.white_space }};

    {{ macros.spacing_margin_all(design.margin) }}
    {{ macros.typography(design.typography, globalSettings) }}
}
%%SELECTOR%% .dan-layer-button__original-layer{
    background-color: {{ content.button_styles.background }};
    color: {{ content.button_styles.text_color }};
    {{ macros.spacing_complex(content.button_styles.padding, 'padding') }}
    {{ macros.border_radius(content.button_styles.border_radius) }}

	--translate3dpxto: {{ content.button_hover_styles.slug_cant_start_with_number_3d_translate_to.style }}; /*from*/
    --color: {{ content.button_hover_styles.text_color }};
    --background: {{ content.button_hover_styles.background }};
    transition-duration: {{ content.button_hover_styles.transition_duration.style }} !important;

    z-index: {{ content.button_styles.zindex }}; /*z-index*/ /**/
    --z-index: {{ content.button_hover_styles.zindex }};
}
%%SELECTOR%% .dan-layer-button__first-layer {
	width: {{ content.first_layer_styles.width.style }}; /**/ /**/
	height: {{ content.first_layer_styles.height.style }}; /**/ /**/
	{{ macros.spacing_complex(content.first_layer_styles.padding, 'padding') }}

	background: {{ content.first_layer_styles.background }}; /**/
	{{ macros.border_radius(content.first_layer_styles.border_radius) }}
    {{ macros.border_complex(content.first_layer_styles.border) }}

	color: {{ content.first_layer_styles.text_color }};

    --translate3dpxfrom: {{ content.first_layer_styles.slug_cant_start_with_number_3d_translate_from.style }}; /*from*/
	--translate3dpxto: {{ content.first_layer_hover_styles.slug_cant_start_with_number_3d_translate_to.style }}; /*from*/

    --color: {{ content.first_layer_hover_styles.text_color }};
    --background: {{ content.first_layer_hover_styles.background }};

    transition-duration: {{ content.first_layer_hover_styles.transition_duration.style }} !important; /*s*/

    z-index: {{ content.first_layer_styles.zindex }}; /*z-index*/ /**/
    --z-index: {{ content.first_layer_hover_styles.zindex }};
}
