%%SELECTOR%%{
    position: {{ design.position.position }};
    z-index: {{ design.position.zindex }};
    top: {{ design.position.top.style }};
    left: {{ design.position.left.style }};
    right: {{ design.position.right.style }};
    bottom: {{ design.position.bottom.style }};

    {{ macros.background(design.background) }}
    {{ macros.spacing_margin_all(design.margin) }}
}

/*
* TASTY HAMBURGERS
*/
%%SELECTOR%%.hamburger {
    --burguer-dimensions: {{ design.burger_styles.dimensions.style }};
    --burguer-stroke-color: {{ design.burger_styles.stroke_color }}; /*stroke color*/
    --burguer-duration: {{ content.animation.duration.style }}; /*animation duration*/
    --burguer-easing: {{ content.animation.css_easing }}; /*animation ease*/

    /*TASTY HAMBURGERS SPECIFIC*/
    --hamburger-layer-spacing: {{ design.burger_styles.stroke_spacing.style }};
    --hamburger-layer-height: {{ design.burger_styles.stroke_height.style }};
    --hamburger-border-radius: {{ design.burger_styles.stroke_radius.style }};
}

/*
* ANIMATED
*/
%%SELECTOR%% .dan-burger-animated{
    --burguer-rounded:{% if design.burger_styles.rounded %}round{% else %}square{% endif %}; /*redondeado*/
    --burguer-stroke-color: {{ design.burger_styles.stroke_color }}; /*stroke color*/
    --burguer-duration: {{ content.animation.duration.style }}; /*animation duration*/
    --burguer-easing: {{ content.animation.css_easing }}; /*animation ease*/

    /*ANIMATED HAMBURGERS SPECIFIC*/
    --burguer-dimensions: {{ design.burger_styles.dimensions.style }};
    --hamburger-layer-height: {{ design.burger_styles.stroke_height.style }};
}

